{
  "name": "typescript-boilerplate",
  "version": "1.0.0",
  "description": "Basic boilerplate project with Typecript",
  "main": "src/main.ts",
  "engineStrict": true,
  "engines": {
    "node": ">= 22"
  },
  "type": "module",
  "license": "ISC",
  "scripts": {
    "start": "node --experimental-transform-types --disable-warning=ExperimentalWarning src/main.ts",
    "test": "node --experimental-transform-types --disable-warning=ExperimentalWarning --test src/**/*test.ts",
    "test:watch": "node --experimental-transform-types --disable-warning=ExperimentalWarning --watch --test src/**/*test.ts"
  },
  "devDependencies": {
    "@types/node": "20.14.12",
    "expect": "^29.7.0",
    "prettier": "3.3.3",
    "typescript": "5.5.4"
  }
}